{
  "name": "boilerplate",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android:dev:debug": "react-native run-android --mode devDebug --appIdSuffix dev",
    "android:production:debug": "react-native run-android --mode productionDebug --appIdSuffix production",
    "android:prod:debug": "react-native run-android --mode productionDebug",
    "android:dev:release": "react-native run-android --mode devRelease --appIdSuffix dev",
    "android:production:release": "react-native run-android --mode productionRelease --appIdSuffix production",
    "android:prod:release": "react-native run-android --mode productionRelease",
    "ios:dev:debug": "react-native run-ios --mode Dev.Debug --scheme \"boilerplateDev\"",
    "ios:production:debug": "react-native run-ios --mode Debug --scheme \"boilerplate\"",
    "ios:prod:debug": "react-native run-ios --mode Debug --scheme \"boilerplate\"",
    "ios:dev:release": "react-native run-ios --mode Dev.Release --scheme \"boilerplateDev\"",
    "ios:production:release": "react-native run-ios --mode Release --scheme \"boilerplate\"",
    "ios:prod:release": "react-native run-ios --mode Release --scheme \"boilerplate\"",
    "start": "react-native start",
    "pods": "cd ci && bundle exec \"cd ../ios/ && pod install --repo-update\"",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "prettier": "prettier --check \"./**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "tsc": "npx tsc --skipLibCheck",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.5",
    "i18next": "^23.14.0",
    "react": "18.3.1",
    "react-i18next": "^15.0.1",
    "react-native": "^0.76.3",
    "react-native-bootsplash": "^6.3.2",
    "react-native-config": "^1.5.3",
    "react-native-device-info": "^11.1.0",
    "react-native-keychain": "^8.2.0",
    "react-native-permissions": "^5.2.1",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^3.29.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.3",
    "@react-native/eslint-config": "0.76.3",
    "@react-native/metro-config": "0.76.3",
    "@react-native/typescript-config": "0.76.3",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "^8.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "husky": "^9.1.5",
    "jest": "^29.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^15.2.10",
    "metro-react-native-babel-preset": "^0.70.3",
    "prettier": "^3.4.2",
    "react-test-renderer": "18.3.1",
    "reactotron-react-native": "^5.1.10",
    "reactotron-redux": "^3.1.10",
    "typescript": "^5.5.4"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [],
    "./**/*.{js,jsx,ts,tsx,json}": []
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
